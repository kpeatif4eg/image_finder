{
    "parser": "babel-eslint",
    "extends": "airbnb",
    "plugins": [
        "react"
    ],
    "rules": {
        "linebreak-style": 0,
        "no-tabs": 0,
        "indent": 0,
        "react/jsx-indent": ["error", 4],
        "react/jsx-filename-extension": 0,
        "react/prop-types": 0,
        "jsx-quotes": 0,
        "no-nested-ternary": 0,
        "no-undef": 0,
        "no-shadow": 0,
        "class-methods-use-this": 0
    },
    "globals": {
        "window": true,
        "__DEV__": true
    }
}
